RewriteEngine on
RewriteRule index.html$ index.php
RewriteRule content.html$ content.php
RewriteRule video.html$ video.php
RewriteRule index_(.*)$ index.php?page=$1
RewriteRule xinggan.html$ xinggan.php
RewriteRule qingchun.html$ qingchun.php
RewriteRule siwa.html$ siwa.php
RewriteRule gif.html$ gif.php
RewriteRule tag.html$ tag.php
RewriteRule xinggan_(.*)$ xinggan.php?page=$1
RewriteRule qingchun_(.*)$ qingchun.php?page=$1
RewriteRule siwa_(.*)$ siwa.php?page=$1
RewriteRule gif_(.*)$ gif.php?page=$1
RewriteRule search.html$ search.php